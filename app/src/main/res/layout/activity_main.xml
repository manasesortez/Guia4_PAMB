<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnllamar"
        android:layout_width="269dp"
        android:layout_height="50dp"
        android:layout_marginTop="92dp"
        android:layout_marginBottom="340dp"
        android:labelFor="@id/rbdmujer"
        android:text="llamar Otro Activity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rbdmujer"
        tools:ignore="HardcodedText,NotSibling,UnknownId" />

    <RadioGroup
        android:layout_width="271dp"
        android:layout_height="238dp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="36dp"
        tools:ignore="MissingConstraints">

        <RadioButton
            android:id="@+id/rbdmujer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mujer"
            tools:ignore="HardcodedText" />

        <RadioButton
            android:id="@+id/rbdhombre"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Hombre"
            tools:ignore="HardcodedText" />
    </RadioGroup>

</androidx.constraintlayout.widget.ConstraintLayout>